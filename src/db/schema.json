	"definitions": {
		"Operation": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"subjectType": {
					"type": "string",
					"enum": [
						"global",
						"issuer",
						"token"
					]
				},
				"subjectId": {
					"type": "integer"
				},
				"task": {
					"type": "string"
				},
				"time": {
					"type": "integer"
				}
			},
			"required": [
				"subjectType",
				"subjectId",
				"task",
				"time"
			],
			"unique": [
				["subjectType", "subjectId", "task"]
			]
		},
		"Snapshot": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"creationTime": {
					"type": "integer"
				},
				"completionTime": {
					"type": "integer"
				},
				"ledgerSequence": {
					"type": "integer"
				},
				"entriesCount": {
					"type": "integer",
					"default": 0
				},
				"originNode": {
					"type": "string"
				},
				"marker": {
					"type": "any"
				}
			},
			"required": [
				"creationTime",
				"ledgerSequence"
			],
			"unique": [
				"ledgerSequence"
			]
		},
		"Ledger": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"sequence": {
					"type": "integer"
				},
				"hash": {
					"type": "string",
					"format": "hex"
				},
				"closeTime": {
					"type": "integer",
					"format": "timestamp"
				},
				"txCount": {
					"type": "integer"
				},
				"txTypeCounts": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/LedgerTxTypeCount"
					}
				},
				"minFee": {
					"type": "integer"
				},
				"maxFee": {
					"type": "integer"
				},
				"avgFee": {
					"type": "integer"
				}
			},
			"required": [
				"sequence",
				"hash",
				"closeTime",
				"txCount"
			],
			"unique": [
				"sequence"
			],
			"index": [
				"closeTime"
			]
		},
		"LedgerTxTypeCount": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"ledger": {
					"$ref": "#/definitions/Ledger"
				},
				"type": {
					"type": "string",
					"enum": [
						"AccountSet",
						"AccountDelete",
						"CheckCancel",
						"CheckCash",
						"CheckCreate",
						"DepositPreauth",
						"EscrowCancel",
						"EscrowCreate",
						"EscrowFinish",
						"NFTokenAcceptOffer",
						"NFTokenBurn",
						"NFTokenCancelOffer",
						"NFTokenCreateOffer",
						"NFTokenMint",
						"OfferCancel",
						"OfferCreate",
						"Payment",
						"PaymentChannelClaim",
						"PaymentChannelCreate",
						"PaymentChannelFund",
						"SetRegularKey",
						"SignerListSet",
						"TicketCreate",
						"TrustSet"
					]
				},
				"count": {
					"type": "integer"
				}
			},
			"unique": [
				["ledger", "type"]
			],
			"required": [
				"ledger",
				"type",
				"count"
			]
		},
		"Account": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"address": {
					"type": "string",
					"format": "xrpl/address"
				},
				"domain": {
					"type": "string",
					"format": "hex"
				},
				"emailHash": {
					"type": "string",
					"format": "hex"
				},
				"transferRate": {
					"type": "integer"
				},
				"blackholed": {
					"type": "boolean"
				},
				"props": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/AccountProp"
					}
				}
			},
			"required": [
				"address"
			],
			"unique": [
				"address"
			]
		},
		"AccountProp": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"account": {
					"$ref": "#/definitions/Account"
				},
				"key": {
					"type": "string"
				},
				"value": {
					"type": "any"
				},
				"source": {
					"type": "string"
				}
			},
			"required": [
				"account",
				"key",
				"source"
			],
			"unique": [
				["account", "key", "source"]
			]
		},
		"AccountBalance": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"account": {
					"$ref": "#/definitions/Account"
				},
				"token": {
					"$ref": "#/definitions/Token"
				},
				"ledgerSequence": {
					"type": "integer"
				},
				"balance": {
					"type": "string",
					"format": "xrpl/xfl",
					"default": 0
				}
			},
			"required": [
				"account",
				"token",
				"ledgerSequence"
			],
			"unique": [
				["account", "token", "ledgerSequence"]
			],
			"index": [
				["token", "ledgerSequence"]
			]
		},
		"Token": {
			"type": "object",
			"properties": {
				"id": {
					"type": "integer",
					"id": true
				},
				"issuer": {
					"$ref": "#/definitions/Account"
				},
				"currency": {
					"type": "string",
					"format": "xrpl/currency"
				}
			},
			"required": [
