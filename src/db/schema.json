				"supplyDelta24H": {
					"type": "string",
					"format": "xrpl/xfl",
					"default": "0"
				},
				"supplyPercent24H": {
					"type": "number",
					"default": 0
				},
				"supplyDelta7D": {
					"type": "string",
					"format": "xrpl/xfl",
					"default": "0"
				},
				"supplyPercent7D": {
					"type": "number",
					"default": 0
				},
				"marketcap": {
					"type": "string",
					"format": "xrpl/xfl",
					"default": "0"
				},
				"marketcapDelta24H": {
					"type": "string",
					"format": "xrpl/xfl",
					"default": "0"
				},
				"marketcapPercent24H": {
					"type": "number",
					"default": 0
				},
				"marketcapDelta7D": {
					"type": "string",
					"format": "xrpl/xfl",
					"default": "0"
				},
				"marketcapPercent7D": {
					"type": "number",
					"default": 0
				},
				"price": {
					"type": "string",
					"format": "xrpl/xfl",
					"default": "0"
				},
				"pricePercent24H": {
					"type": "number",
					"default": 0
				},
				"pricePercent7D": {
					"type": "number",
					"default": 0
				},
				"volume24H": {
					"type": "string",
					"format": "xrpl/xfl",
					"default": "0"
				},
				"volume7D": {
					"type": "string",
					"format": "xrpl/xfl",
					"default": "0"
				}
			},
			"required": [
				"token"
			],
			"unique": [
				"token"
			],
			"index": [
				"trusted",
				"trustlines",
				"trustlinesDelta24H",
				"trustlinesPercent24H",
				"trustlinesDelta7D",
				"trustlinesPercent7D",
				"holders",
				"holdersDelta24H",
				"holdersPercent24H",
				"holdersDelta7D",
				"holdersPercent7D",
				"supply",
				"supplyDelta24H",
				"supplyPercent24H",
				"supplyDelta7D",
				"supplyPercent7D",
				"marketcap",
				"marketcapDelta24H",
				"marketcapPercent24H",
				"marketcapDelta7D",
				"marketcapPercent7D",
				"price",
				"pricePercent24H",
				"pricePercent7D",
				"volume24H",
				"volume7D"
