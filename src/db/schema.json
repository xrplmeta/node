			}
		},
		"tokens": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/Token"
			}
		},
		"tokenCache": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/TokenCache"
			}
		},
		"tokenProps": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/TokenProp"
			}
		},
		"tokenExchanges": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/TokenExchange"
			}
		},
		"tokenSupply": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/TokenSupply"
			}
		},
		"tokenMarketcap": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/TokenMarketcap"
			}
		},
		"tokenTrustlines": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/TokenTrustlines"
			}
		},
		"tokenHolders": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/TokenHolders"
			}
		},
		"tokenBooks": {
			"type": "array",
			"items": {
